extends layout

block content
  h1= title
  p Welcome to index Pradeep Rao Veeramaneni  #{title}
  button(type="button", class="btn btn-danger" onclick="doBump()") Press me
  div(id="bump") Bump the count
  script.
   var count=0;
   var bump=1;
   function doBump(){
    count+=bump;
    bump+=1;
    document.getElementById("bump").innerHTML="Count "+count+" Bump "+bump;
   }
